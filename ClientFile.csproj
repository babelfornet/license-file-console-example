    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Babel.Licensing" Version="10.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Update="ClientFile.licenses">
